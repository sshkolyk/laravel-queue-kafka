{
  "name": "sshkolyk/laravel-queue-kafka",
  "description": "Kafka driver for Laravel Queue",
  "license": "MIT",
  "type": "library",
  "version": "2.0",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/database": "^12.0 || ^13.0",
    "illuminate/support": "^12.0 || ^13.0",
    "illuminate/queue": "^12.0 || ^13.0",
    "ext-rdkafka": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "laravel/pint": "^1.27"
  },
  "autoload": {
    "psr-4": {
      "Rapide\\LaravelQueueKafka\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rapide\\LaravelQueueKafka\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rapide\\LaravelQueueKafka\\LaravelQueueKafkaServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
